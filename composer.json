{
  "name": "plab/crudy",
  "require": {
    "php": ">=7.0",
    "hoa/router": "~3.0",
    "hoa/dispatcher": "~1.0",
    "hoa/view": "~3.0",
    "hoa/exception": "~1.0",
    "ihor/nspl": "~1.0",
    "plab/value-object": "~1.0.0"
  },
  "require-dev": {
    "codeception/codeception": "~2.2",
    "codeception/c3": "~2.0",
    "psy/psysh": "@stable",
    "flow/jsonpath": "@stable"
  },
  "autoload": {
    "psr-4": {
      "Crudy\\": "src/"
    },
    "exclude-from-classmap": ["/public/", "/tests/"]
  },
  "autoload-dev": {
    "psr-4": {
      "CrudyApplication\\": "public/"
    }
  }
}
